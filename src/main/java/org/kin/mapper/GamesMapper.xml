<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kin.mapper.GamesMapper">

    <!--根据id查询数据-->
    <select id="findGamesById" parameterType="int" resultType="Games">
        select k as id,vl as name from games where k = #{id}
    </select>

    <!--查询所有数据-->
    <select id="findAllGames" resultType="Games">
        SELECT  k as id,vl as name  from games
    </select>


</mapper>